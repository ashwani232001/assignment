public class Elseif {
    public static void main(String[] args) {
      int n=0;

      if (n<0){
          System.out.println(n+" number is negative");
      } else if (n>0) {
          System.out.println(n+" number is positive");
      }
      else{
          System.out.println("number is zero");
      }
    }
}
